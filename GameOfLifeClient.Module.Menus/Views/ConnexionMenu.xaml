<UserControl x:Class="GameOfLifeClient.Module.Menus.Views.ConnexionMenu"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:GameOfLifeClient.Module.Menus.Views"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <StackPanel>
        <TextBox Text="{Binding MyName,Mode=TwoWay}" />
        <Button Content="Connexion" Command="{Binding Connexion}"></Button>
    </StackPanel>
</UserControl>
